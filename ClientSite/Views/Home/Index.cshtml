@{
    ViewData["Title"] = "Home Page";
}

<head>
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="~/css/color.css">
    <script src="~/js/jquery-3.4.1.js"></script>
    <script src="~/js/site.js"></script>
</head>

<input id="guid" value="a" hidden>
<input id="idPos" value="0" hidden>

<section>
    <div id="display" class="left">
    </div>
    <div class="mapHolder left">
        <img id="map" class="left" src="~/map.jpg" />
        @*<img id="mapOverlay" class="left" src="~/6-1.png" />*@
        <img id="mapOverlay" class="left" />
    </div>

    <span class="WideSpan">
        <div id="info" class="left">
            <pre class="Health Left LeftColumn">Health </pre>
            <pre id="HealthStatus" class="Health Left">10/100</pre>
            <pre class="LineBreak"></pre>

            <pre class="Mana Left LeftColumn">Mana </pre>
            <pre id="ManaStatus" class="Mana Left">10/100</pre>
            <pre class="LineBreak"></pre>

            <pre class="Stamina Left LeftColumn">Stamina </pre>
            <pre id="StaminaStatus" class="Stamina Left">10/100</pre>
            <pre class="LineBreak"></pre>
        </div>
    </span>
</section>

<div class="Footer">

    <div class="Left">Input</div>
    <span class="WideSpan">
        <input id="input" class="right" />
    </span>
</div>



